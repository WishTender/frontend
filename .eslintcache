mageUpdate = (img) => {\n    setImageFile(img);\n  };\n  const deleteWish = () => {\n    fetchDelete(`/wishlistItems/${props.id}`, props.onClose());\n  };\n  return (\n    <form\n      autocomplete=\"off\"\n      style={props.disabled ? { opacity: \".3\", pointerEvents: \"none\" } : {}}\n      className={classes.root}\n      onSubmit={handleSubmit(onSubmit)}\n    >\n      <Container>\n        <img\n          src={\n            // should we URL.revokeObjectURL();?\n\n            imageFile ? URL.createObjectURL(imageFile) : props.info.itemImage\n          }\n          alt=\"product\"\n        />\n\n        <SelectCropUpdateImage\n          aspect={1}\n          cropShape=\"rect\"\n          finalImageDimensions={{ width: 300, height: 300 }}\n          handleUpdateImage={handleImageUpdate}\n        >\n          <p style={{ textDecoration: \"underline\", fontSize: \".8em\" }}>\n            Upload Custom Photo\n          </p>\n        </SelectCropUpdateImage>\n      </Container>\n      <Typography>Edit Wish Info</Typography>\n      <FormControl error={errors.itemName ? true : false}>\n        <TextField\n          inputRef={register({\n            maxLength: {\n              value: 199,\n              message: \"Product name must be less than 200 characters\",\n            },\n            minLength: {\n              value: 5,\n              message: \"Product name must be more than 4 characters\",\n            },\n          })}\n          name=\"itemName\"\n          variant=\"outlined\"\n          value={itemName}\n          label=\"Product Name\"\n          onChange={(e) => {\n            setItemName(e.target.value);\n          }}\n        />\n        <FormHelperText>{errors.itemName?.message}</FormHelperText>\n      </FormControl>\n\n      <PriceInput\n        price={price}\n        setPrice={setPrice}\n        inputRef={register({\n          validate: (value) => {\n            return (\n              !isNaN(+toDecimal(value)) || `${value} is not a valid price.`\n            );\n          },\n        })}\n        error={errors.price?.message}\n        symbol={getSymbol(props.info.currency)}\n      ></PriceInput>\n\n      <Grid container justify=\"flex-end\">\n        <StyledDialog\n          open={deleteWarningVisible}\n          onClose={() => setDeleteWarningVisible(false)}\n        >\n          <p>Are you sure you want to delete this wish?</p>\n          <Button onClick={() => setDeleteWarningVisible(false)}>No</Button>\n          <Button\n            onClick={() => {\n              setDeleteWarningVisible(false);\n              deleteWish();\n            }}\n          >\n            Yes\n          </Button>\n        </StyledDialog>\n        <Button size=\"small\" onClick={() => setDeleteWarningVisible(true)}>\n          Delete Wish\n        </Button>\n      </Grid>\n\n      <Button\n        disableElevation={true}\n        className={classes.button}\n        variant=\"contained\"\n        color=\"primary\"\n        size=\"large\"\n        type=\"submit\"\n      >\n        Update\n      </Button>\n    </form>\n  );\n}\n","/Users/dashiellbarkhuss/Documents/gift_registry_business_idea/react app/wishtender/src/components/SignUp/SignUp.js",["262"],"import React, { useState, useEffect } from \"react\";\nimport { useForm } from \"react-hook-form\";\nimport { Redirect } from \"react-router-dom\";\n\nexport default function SignUp() {\n  const { register, handleSubmit, errors } = useForm();\n  const [success, setSuccess] = useState(0);\n  // useEffect(() => {\n  //   console.log(\"effect\");\n  // });\n\n  const onSubmit = (data) => {\n    console.log(data);\n    const headers = new Headers();\n    headers.append(\"Content-Type\", \"application/json\");\n    // send the country code to the server where we will also detect the browser's preferred language located in the acceptsLanguages request header\n    fetch(\"/users/registration\", {\n      method: \"POST\",\n      body: JSON.stringify(data),\n      headers,\n    })\n      .then(